<h1>Task#index</h1>
<div>
  <ul>
    <% @tasks.each do |task| %>
      <li>
        <%= task.title %>
        <%= link_to 'Show', task %>
        <%= link_to 'Edit', edit_task_path(task) %>
        <%= link_to 'Ajax Edit', ajax_edit_task_path(task) %>
        <%= link_to 'Destroy', task, method: :delete %>
      </li>
    <% end %>
  </ul>

  <table>
    <thead>
      <tr>
        <td>Title</td>
        <td>Description</td>
      </tr>
    </thead>


  </table>

  <%= link_to 'New', new_task_path %>
</div>